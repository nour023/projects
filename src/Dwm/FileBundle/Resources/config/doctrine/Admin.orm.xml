<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FileBundle\Entity\Admin" table="admin">
    <id name="idAdmin" type="integer" column="id_admin">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="50" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="login" type="string" column="login" length="50" nullable="false"/>
    <field name="pwd" type="string" column="pwd" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
